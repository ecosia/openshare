{
  "extends": "airbnb",
  "rules": {
    "new-cap": [0, { "capIsNew": true }],
    "max-len": [2, 200],
    "no-plusplus": 0,
    "no-mixed-operators": 0,
    "no-alert": 0,
    "no-restricted-syntax": 0,
    "no-param-reassign": 0,
    "no-new": 0,
    "no-console": 0,
    "global-require": 0,
    "one-var": 0,
    "consistent-return": 0,
    "no-underscore-dangle": 0,
    "no-use-before-define": 0,
    "no-shadow": 0
  },
  "env": {
    "browser": true,
    "node": true
  }
}
